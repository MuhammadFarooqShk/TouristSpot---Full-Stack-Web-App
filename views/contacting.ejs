<head>
    <link rel="stylesheet" href="jqstyles4.css">
  </head>
            
  <h1 class="self1">Contact<br><span>User Messages List</span></h1>    </header>
<br>
<div class="container">
    <div class="row">
        <% contacts.forEach(function(contact) { %>
        <div class="col-md-4 mb-4">
            <div class="card h-100">
                <div class="card-body">
                    <h5 class="card-title" style="text-align: center;"><%= contact.name %></h5>
                    <p class="card-text"><strong>Email:</strong> <%= contact.email %></p>
                    <p class="card-text"><strong>Message:</strong> <%= contact.message %></p>
                    <p class="card-text"><small class="text-muted"><%= contact.createdAt.toLocaleString() %></small></p>
                </div>
            </div>
        </div>
        <% }); %>
    </div>
</div>